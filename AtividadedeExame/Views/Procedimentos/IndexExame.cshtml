@model IEnumerable<Modelo.Exame>

@{
    ViewBag.Title = "IndexExame";
    Layout = null;
}

<h2>IndexExame</h2>

<p>
    @Html.ActionLink("Create New", "ExameCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.descricao)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.descricao)
        </td>
        <td>
            @Html.ActionLink("Edit", "ExameEdit", new { id=item.Id }) |
            @Html.ActionLink("Details", "ExameDetails", new { id=item.Id }) |
            @Html.ActionLink("Delete", "ExameDelete", new { id=item.Id })
        </td>
    </tr>
}

</table>
